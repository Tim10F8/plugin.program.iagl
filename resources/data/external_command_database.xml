<system>
<!-- OSX External Retroarch Commands -->
	<launcher name="" os="OSX" default="true">
		<command>"%APP_PATH_RA%" %NETPLAY_COMMAND% -L "%CORE_PATH%" "%ROM_PATH%"</command>
	</launcher>
<!-- OSX Additional Emulator External Commands -->
	<launcher name="Dolphin (Wii/Gamecube)" os="OSX">
		<command>"%APP_PATH_DOLPHIN%" --exec="%ROM_PATH%" --batch</command>
	</launcher>
	<launcher name="FS-UAE Launcher" os="OSX">
		<command>"%APP_PATH_FS_UAE%" "%ROM_PATH%"</command>
	</launcher>
	<launcher name="MAME Standalone (Arcade)" os="OSX">
		<command>"%APP_PATH_MAME%" "%ROM_PATH%"</command>
	</launcher>
	<launcher name="RetroArch Launch App Only" os="OSX">
		<command>"%APP_PATH_RA%"</command>
	</launcher>
<!-- OSX External Retroarch Commands, with script to close kodi before launch -->
	<launcher name="" os="OSX" default="true" close_kodi="true">
		<command>"%ADDON_DIR%/resources/bin/applaunch.sh" "%APP_PATH_RA%" %NETPLAY_COMMAND% -L "%CORE_PATH%" "%ROM_PATH%"</command>
	</launcher>
